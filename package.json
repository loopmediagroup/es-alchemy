{
  "name": "es-alchemy",
  "version": "0.0.0-development",
  "description": "Simplification of Elasticsearch interactions",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npx babel src --out-dir lib --copy-files --include-dotfiles --config-file ./.babelrc",
    "build-clean": "yarn run clean && yarn run build",
    "test-simple": "nyc mocha \"./test/**/*.spec.js\"",
    "test": "yarn run clean && yarn run gardener && yarn run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "semantic-release": "yarn run build-clean && npx semantic-release",
    "gardener": "node gardener",
    "t": "yarn test",
    "ts": "yarn run test-simple",
    "tsv": "yarn run test-simple --verbose",
    "docker": "docker run --net host -u`id -u`:`id -g` -v $(pwd):/user/project -v ~/.aws:/user/.aws -v ~/.npmrc:/user/.npmrc -w /user/project -it --entrypoint /bin/bash",
    "u": "yarn upgrade --latest --force",
    "i": "yarn install --frozen-lockfile",
    "it": "yarn run i && yarn run t"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loopmediagroup/es-alchemy.git"
  },
  "keywords": [
    "elasticsearch",
    "abstraction",
    "simplification",
    "sdk",
    "query",
    "filter",
    "builder"
  ],
  "author": "Loop Media Group",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loopmediagroup/es-alchemy/issues"
  },
  "homepage": "https://github.com/loopmediagroup/es-alchemy#readme",
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/register": "7.16.0",
    "@blackflux/eslint-plugin-rules": "2.0.4",
    "@blackflux/robo-config-plugin": "5.3.7",
    "babel-eslint": "10.1.0",
    "babel-preset-latest-node": "5.5.1",
    "chai": "4.3.4",
    "coveralls": "3.1.1",
    "deep-equal-in-any-order": "1.1.15",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-mocha": "9.0.0",
    "js-gardener": "3.0.3",
    "node-tdd": "3.1.0",
    "nyc": "15.1.0",
    "request": "2.88.2",
    "request-promise-native": "1.0.9",
    "semantic-release": "17.4.4",
    "uuid": "8.3.2"
  },
  "nyc": {
    "tempDir": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true,
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "lib/*"
    ]
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/loopmediagroup/es-alchemy/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "joi-strict": "2.0.1",
    "lodash.clonedeep": "4.5.0",
    "lodash.get": "4.4.2",
    "lodash.has": "4.5.2",
    "lodash.isequal": "4.5.0",
    "lodash.set": "4.3.2",
    "object-fields": "3.0.1",
    "object-hash": "2.2.0",
    "object-scan": "17.1.0",
    "smart-fs": "2.0.3"
  },
  "peerDependencies": {
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5"
  }
}
