{
  "name": "es-alchemy",
  "type": "module",
  "version": "0.0.0-development",
  "description": "Simplification of Opensearch interactions",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "cp -rf ./src ./lib",
    "build-clean": "yarn run clean && yarn run build",
    "test-simple": "c8 mocha --import=./test/reg.js \"./test/**/*.spec.js\"",
    "test": "yarn run clean && yarn run gardener && yarn run test-simple",
    "semantic-release": "yarn run build-clean && yarn exec semantic-release",
    "gardener": "node gardener.js",
    "t": "yarn test",
    "ts": "yarn run test-simple",
    "tsv": "yarn run test-simple --verbose",
    "docker": "docker run --net host -u`id -u`:`id -g` -v $(pwd):/user/project -v ~/.aws:/user/.aws -v ~/.npmrc:/user/.npmrc -w /user/project -it --entrypoint /bin/bash",
    "u": "yarn upgrade --latest --force",
    "i": "yarn install --frozen-lockfile",
    "it": "yarn run i && yarn run t"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loopmediagroup/es-alchemy.git"
  },
  "keywords": [
    "elasticsearch",
    "opensearch",
    "abstraction",
    "simplification",
    "sdk",
    "query",
    "filter",
    "builder"
  ],
  "author": "Loop Media Group",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loopmediagroup/es-alchemy/issues"
  },
  "homepage": "https://github.com/loopmediagroup/es-alchemy#readme",
  "devDependencies": {
    "@babel/core": "7.28.0",
    "@babel/eslint-parser": "7.28.0",
    "@babel/register": "7.27.1",
    "@blackflux/eslint-plugin-rules": "4.0.1",
    "@blackflux/robo-config-plugin": "10.0.6",
    "c8": "10.1.3",
    "chai": "5.2.1",
    "deep-equal-in-any-order": "2.0.6",
    "eslint": "9.31.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-mocha": "10.5.0",
    "js-gardener": "6.0.4",
    "node-tdd": "6.2.1",
    "uuid": "11.1.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/loopmediagroup/es-alchemy/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 16"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@blackflux/axios": "3.0.2",
    "aws4-axios": "3.4.0",
    "joi-strict": "4.0.1",
    "lodash.clonedeep": "4.5.0",
    "lodash.get": "4.4.2",
    "lodash.has": "4.5.2",
    "lodash.isequal": "4.5.0",
    "lodash.set": "4.3.2",
    "object-fields": "5.0.0",
    "object-hash": "3.0.0",
    "object-scan": "20.0.1",
    "smart-fs": "5.0.2"
  },
  "peerDependencies": {}
}
