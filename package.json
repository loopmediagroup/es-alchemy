{
  "name": "es-alchemy",
  "version": "0.0.0-development",
  "description": "Simplification of Elasticsearch interactions",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npx babel src --out-dir lib --copy-files --include-dotfiles --config-file ./.babelrc",
    "build-clean": "npm run clean && npm run build",
    "test-simple": "nyc mocha \"./test/**/*.spec.js\"",
    "test": "npm run clean && npm run gardener && npm run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "semantic-release": "npm run build-clean && npx semantic-release",
    "gardener": "node gardener"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loopmediagroup/es-alchemy.git"
  },
  "keywords": [
    "elasticsearch",
    "abstraction",
    "simplification",
    "sdk",
    "query",
    "filter",
    "builder"
  ],
  "author": "Loop Media Group",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loopmediagroup/es-alchemy/issues"
  },
  "homepage": "https://github.com/loopmediagroup/es-alchemy#readme",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/register": "7.4.4",
    "@blackflux/eslint-plugin-rules": "1.0.19",
    "@blackflux/robo-config-plugin": "1.10.6",
    "babel-eslint": "10.0.2",
    "chai": "4.2.0",
    "coveralls": "3.0.4",
    "deep-equal-in-any-order": "1.0.13",
    "eslint": "5.16.0",
    "js-gardener": "2.0.40",
    "nyc": "14.1.1",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "semantic-release": "15.13.16",
    "uuid": "3.3.2"
  },
  "nyc": {
    "tempDir": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true,
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "lib/*"
    ]
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/loopmediagroup/es-alchemy/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 8.10"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "lodash.clonedeep": "4.5.0",
    "lodash.get": "4.4.2",
    "lodash.isequal": "4.5.0",
    "object-fields": "1.3.8",
    "object-hash": "1.3.1",
    "object-scan": "10.0.3"
  },
  "peerDependencies": {
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5"
  }
}
